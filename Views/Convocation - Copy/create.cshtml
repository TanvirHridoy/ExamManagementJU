@model CertificationMS.ContextModels.Department

@{
    ViewData["Title"] = "create";
}
<h3>Department</h3>
<form asp-action="Add" class="col-md-8">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="DeptName" for="exampleInputEmail1">Department Name</label>
        <input asp-for="DeptName" class="form-control" id="exampleInputEmail1" placeholder="Enter Department Name" aria-describedby="emailHelp">
        <span asp-validation-for="DeptName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="DeptSname" for="exampleInputPassword1">Department Short Name</label>
        <input asp-for="DeptSname" class="form-control" placeholder="Enter Department Short Name" id="exampleInputPassword1">
        <span asp-validation-for="DeptSname" class="text-danger"></span>
    </div>


    <div class="form-group">
        <input type="submit" asp-action="Add" value="Create" class="btn btn-primary" />
    </div>
</form>
<div class="card-footer text-muted">
    <div>
        <a asp-action="List">Back to List</a>
    </div>
</div>






